server {
    listen {{default_port}};
    server_name {{domain_name}};

    location / {
        proxy_set_header Host $host;
        proxy_pass {{grafana_admin_url}};
    }

    location /api/live/ {
        proxy_http_version 1.1;
        proxy_set_header Host $host;
        proxy_pass {{grafana_admin_url}};
    }
}

